{
  "name": "factory-erp-server",
  "version": "1.0.0",
  "description": "Complete Factory ERP Server with TypeScript, MongoDB, and Advanced Security",
  "main": "dist/start.js",
  "_moduleAliases": {
    "@": "dist",
    "@/config": "dist/config",
    "@/controllers": "dist/controllers",
    "@/middleware": "dist/middleware",
    "@/models": "dist/models",
    "@/routes": "dist/routes",
    "@/services": "dist/services",
    "@/utils": "dist/utils",
    "@/types": "dist/types",
    "@/validators": "dist/validators"
  },
  "scripts": {
    "dev": "dotenvx run --env-file=.env.local -- nodemon src/server.ts",
    "build": "tsc",
    "start": "node -r tsconfig-paths/register dist/start.js",
    "start:prod": "NODE_ENV=production node -r tsconfig-paths/register dist/start.js",
    "test": "jest",
    "seed:vehicles": "node seeders/vehicleSeeder.js",
    "seed:hospitality": "node seeders/hospitalitySeeder.js",
    "seed:inventory": "node seeders/inventorySeeder.js",
    "seed:all": "node seeders/runAllSeeders.js",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "db:seed": "ts-node src/scripts/seedDatabase.ts",
    "db:seed:simple": "ts-node -r tsconfig-paths/register src/scripts/simple-seed.ts",
    "db:seed:user-companies": "ts-node -r tsconfig-paths/register src/scripts/seedUserCompanies.ts",
    "db:seed:comprehensive": "ts-node -r tsconfig-paths/register src/scripts/comprehensive-seed.ts",
    "db:migrate": "ts-node src/scripts/migrate.ts",
    "health": "curl http://localhost:4000/health"
  },
  "keywords": [
    "erp",
    "factory",
    "typescript",
    "mongodb",
    "express",
    "security"
  ],
  "author": "Factory ERP Team",
  "license": "MIT",
