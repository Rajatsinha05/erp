#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/rajat/Downloads/projects/erp/server/node_modules/.pnpm/tshy@1.18.0/node_modules/tshy/dist/esm/node_modules:/Users/rajat/Downloads/projects/erp/server/node_modules/.pnpm/tshy@1.18.0/node_modules/tshy/dist/node_modules:/Users/rajat/Downloads/projects/erp/server/node_modules/.pnpm/tshy@1.18.0/node_modules/tshy/node_modules:/Users/rajat/Downloads/projects/erp/server/node_modules/.pnpm/tshy@1.18.0/node_modules:/Users/rajat/Downloads/projects/erp/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/rajat/Downloads/projects/erp/server/node_modules/.pnpm/tshy@1.18.0/node_modules/tshy/dist/esm/node_modules:/Users/rajat/Downloads/projects/erp/server/node_modules/.pnpm/tshy@1.18.0/node_modules/tshy/dist/node_modules:/Users/rajat/Downloads/projects/erp/server/node_modules/.pnpm/tshy@1.18.0/node_modules/tshy/node_modules:/Users/rajat/Downloads/projects/erp/server/node_modules/.pnpm/tshy@1.18.0/node_modules:/Users/rajat/Downloads/projects/erp/server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../tshy@1.18.0/node_modules/tshy/dist/esm/index.js" "$@"
else
  exec node  "$basedir/../../../../../../tshy@1.18.0/node_modules/tshy/dist/esm/index.js" "$@"
fi
