#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/rajat/Downloads/projects/erp/server/node_modules/.pnpm/brfs@2.0.2/node_modules/brfs/bin/node_modules:/Users/rajat/Downloads/projects/erp/server/node_modules/.pnpm/brfs@2.0.2/node_modules/brfs/node_modules:/Users/rajat/Downloads/projects/erp/server/node_modules/.pnpm/brfs@2.0.2/node_modules:/Users/rajat/Downloads/projects/erp/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/rajat/Downloads/projects/erp/server/node_modules/.pnpm/brfs@2.0.2/node_modules/brfs/bin/node_modules:/Users/rajat/Downloads/projects/erp/server/node_modules/.pnpm/brfs@2.0.2/node_modules/brfs/node_modules:/Users/rajat/Downloads/projects/erp/server/node_modules/.pnpm/brfs@2.0.2/node_modules:/Users/rajat/Downloads/projects/erp/server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../brfs/bin/cmd.js" "$@"
else
  exec node  "$basedir/../brfs/bin/cmd.js" "$@"
fi
