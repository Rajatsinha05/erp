#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/rajat/Downloads/projects/erp/server/node_modules/.pnpm/tap@19.2.5_@types+node@20.19.9_react-dom@18.3.1_react@18.3.1_typescript@5.8.3/node_modules/tap/dist/esm/node_modules:/Users/rajat/Downloads/projects/erp/server/node_modules/.pnpm/tap@19.2.5_@types+node@20.19.9_react-dom@18.3.1_react@18.3.1_typescript@5.8.3/node_modules/tap/dist/node_modules:/Users/rajat/Downloads/projects/erp/server/node_modules/.pnpm/tap@19.2.5_@types+node@20.19.9_react-dom@18.3.1_react@18.3.1_typescript@5.8.3/node_modules/tap/node_modules:/Users/rajat/Downloads/projects/erp/server/node_modules/.pnpm/tap@19.2.5_@types+node@20.19.9_react-dom@18.3.1_react@18.3.1_typescript@5.8.3/node_modules:/Users/rajat/Downloads/projects/erp/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/rajat/Downloads/projects/erp/server/node_modules/.pnpm/tap@19.2.5_@types+node@20.19.9_react-dom@18.3.1_react@18.3.1_typescript@5.8.3/node_modules/tap/dist/esm/node_modules:/Users/rajat/Downloads/projects/erp/server/node_modules/.pnpm/tap@19.2.5_@types+node@20.19.9_react-dom@18.3.1_react@18.3.1_typescript@5.8.3/node_modules/tap/dist/node_modules:/Users/rajat/Downloads/projects/erp/server/node_modules/.pnpm/tap@19.2.5_@types+node@20.19.9_react-dom@18.3.1_react@18.3.1_typescript@5.8.3/node_modules/tap/node_modules:/Users/rajat/Downloads/projects/erp/server/node_modules/.pnpm/tap@19.2.5_@types+node@20.19.9_react-dom@18.3.1_react@18.3.1_typescript@5.8.3/node_modules:/Users/rajat/Downloads/projects/erp/server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../tap@19.2.5_@types+node@20.19.9_react-dom@18.3.1_react@18.3.1_typescript@5.8.3/node_modules/tap/dist/esm/run.mjs" "$@"
else
  exec node  "$basedir/../../../../../tap@19.2.5_@types+node@20.19.9_react-dom@18.3.1_react@18.3.1_typescript@5.8.3/node_modules/tap/dist/esm/run.mjs" "$@"
fi
