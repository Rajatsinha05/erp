#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/rajat/Downloads/projects/erp/server/node_modules/.pnpm/@isaacs+ts-node-temp-fork-for-pr-2009@10.9.7_@types+node@20.19.9_typescript@5.8.3/node_modules/@isaacs/ts-node-temp-fork-for-pr-2009/dist/node_modules:/Users/rajat/Downloads/projects/erp/server/node_modules/.pnpm/@isaacs+ts-node-temp-fork-for-pr-2009@10.9.7_@types+node@20.19.9_typescript@5.8.3/node_modules/@isaacs/ts-node-temp-fork-for-pr-2009/node_modules:/Users/rajat/Downloads/projects/erp/server/node_modules/.pnpm/@isaacs+ts-node-temp-fork-for-pr-2009@10.9.7_@types+node@20.19.9_typescript@5.8.3/node_modules/@isaacs/node_modules:/Users/rajat/Downloads/projects/erp/server/node_modules/.pnpm/@isaacs+ts-node-temp-fork-for-pr-2009@10.9.7_@types+node@20.19.9_typescript@5.8.3/node_modules:/Users/rajat/Downloads/projects/erp/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/rajat/Downloads/projects/erp/server/node_modules/.pnpm/@isaacs+ts-node-temp-fork-for-pr-2009@10.9.7_@types+node@20.19.9_typescript@5.8.3/node_modules/@isaacs/ts-node-temp-fork-for-pr-2009/dist/node_modules:/Users/rajat/Downloads/projects/erp/server/node_modules/.pnpm/@isaacs+ts-node-temp-fork-for-pr-2009@10.9.7_@types+node@20.19.9_typescript@5.8.3/node_modules/@isaacs/ts-node-temp-fork-for-pr-2009/node_modules:/Users/rajat/Downloads/projects/erp/server/node_modules/.pnpm/@isaacs+ts-node-temp-fork-for-pr-2009@10.9.7_@types+node@20.19.9_typescript@5.8.3/node_modules/@isaacs/node_modules:/Users/rajat/Downloads/projects/erp/server/node_modules/.pnpm/@isaacs+ts-node-temp-fork-for-pr-2009@10.9.7_@types+node@20.19.9_typescript@5.8.3/node_modules:/Users/rajat/Downloads/projects/erp/server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@isaacs+ts-node-temp-fork-for-pr-2009@10.9.7_@types+node@20.19.9_typescript@5.8.3/node_modules/@isaacs/ts-node-temp-fork-for-pr-2009/dist/bin-esm.js" "$@"
else
  exec node  "$basedir/../../../../../../@isaacs+ts-node-temp-fork-for-pr-2009@10.9.7_@types+node@20.19.9_typescript@5.8.3/node_modules/@isaacs/ts-node-temp-fork-for-pr-2009/dist/bin-esm.js" "$@"
fi
